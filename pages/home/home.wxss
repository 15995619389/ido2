/**index.wxss**/
.page{
  position: relative;
  width: 100%;
}
.page-header{
  overflow: hidden;
  width: 100%;
  height: 418rpx;
  padding: 0;
  margin: 0;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
  background: #fff;
}
.index_banner{
  width: 100%;
  height: 280rpx;
}
.user_item{
  /* padding-right: 30rpx; */
  position: absolute;
  top: 104rpx;
  left: 40rpx;
  height: 100rpx;
  width: 90%;
  /* line-height: 1.5; */
  display: table;
  font-size: 32rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.user_img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 30rpx;
  float: left;
}
.user_item .user_name{
  display: table-cell;
  vertical-align:middle;
  text-align:left;
  float: left;
  width: 545rpx;
  margin-top: 10rpx;
}
.user_item .user_no_name{
  text-align:left;
  float: left;
  width: 545rpx;
  line-height: 104rpx;
}
/*  */
.page-content{
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 418rpx;
  left: 0;
  right: 0;
  bottom: 104rpx;
  border-bottom: 2rpx solid #F4F4F4;
}
.calendar_header{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  background: #fff;
}
.top{
  height: 82rpx;
  line-height: 82rpx;
  padding: 0 40rpx;
}
.calendar_header text{
  float: left;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-size: 32rpx;
  font-weight:500;
  color:rgba(57,57,57,1);
}
.calendar_header .picker{
  float: right;
  color: #373737;
  font-size: 28rpx;
}
.separator{
  width: 100%;
  height: 8rpx;
  background: #f4f4f4;
}
.calendar_body{
  overflow-y: scroll;
  width: 100%;
  height: 100%;
}
.week{
  display: flex;
  justify-content: center;
  width: 100%;
}
.week view{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  text-align: center;
  line-height: 76rpx;
  font-size: 24rpx;
  font-weight:bold;
  color:rgba(109,109,109,1);
  background: #F4F4F4;
}

/* 日历 */
.days{
  border-bottom: 1px solid #ddd;
}
.columns{
  display: flex;
  flex-direction: column;
  justify-content: space-between; 
}
.columns .rows{
  border-bottom: 1px solid #ddd;
}
.rows_item{
  border-right: 1px solid #ddd;
  float: left;
  width: 14%;
}
.rows_item:last-child{
  border-right: none;
}
.columns .rows .cell{
  height: 130rpx;
  padding-top: 10rpx;
  text-align: center;
}
.day_text{
  font-size:  32rpx;
  font-family:PingFang-SC-Bold,PingFang-SC;
  font-weight:bold;
  color:rgba(77,77,77,1);
  line-height:44rpx;
  height: 44rpx;
}

.count .daynumber{
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.count .daynumber .day{
  margin-top: 50rpx;
}
.count{
  margin: 20rpx;
  padding: 30rpx;
  display: flex;
  text-align: center;
  border-radius: 10px;
  flex-direction: column;
  justify-content: center;
  background-color: #acd;
  align-items: center;
}
.count .number{
  color: red;
  font-size: 60rpx;
  background-color: #fff;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 20rpx;
}
.count text{
  margin: 10rpx;
}
/*  */
.calendar_footer{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  overflow: hidden;
  padding: 40rpx 140rpx;
}
.footer_item{
  float: left;
  margin-right: 50rpx;
  height: 24rpx;
  line-height: 24rpx;
  color: #030303;
  font-size: 20rpx;
}
.footer_item:last-child{
  margin-right: 0;
}
.round{
  width: 24rpx;
  height: 24rpx;
  float: left;
  margin-right: 10rpx;
  /* background: #f7b500;
  border-radius: 50%; */
}
.round2{
  background: #ff88b6;
}
.round3{
  background: #00c1ff;
}
.round4{
  background: #10cd66;
}
.statu_feiji,.statu_banlv,.statu_mengyi{
  width: 100%;
  position: absolute;
  left: 0;
  right: 0;
}
.statu_banlv{
  top: 54rpx;
}
.statu_feiji{
  top: 82rpx;
}
.statu_mengyi{
  top: 110rpx;
}

.statu{
  position: relative;
  width: 100%;
  height: 24rpx;
  margin-bottom: 4rpx;
}

.small_icon{
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -12rpx;
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
}
.statu1_1{
  /* float: right; */
  position: absolute;
  top: 4rpx;
  right: -1px;
  width: 32rpx;
  height: 16rpx;
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
}
.statu1_2{
  position: absolute;
  top: 4rpx;
  left: -1px;
  right: 0;
  width: 102%;
  height: 16rpx;
}
.statu1_3{
  position: absolute;
  top: 4rpx;
  left: -2rpx;
  width: 71rpx;
  height: 16rpx;
  border-top-right-radius: 10px;
  border-bottom-right-radius: 10px;
}
.statu1_3 .wite_round{
  float: right;
  margin-right: 4rpx;
  margin-top: 4rpx;
}
.statu1_1 .wite_round{
  float: left;
  margin-top: 4rpx;
  margin-left: 4rpx;
}
.wite_round{
  width: 4px;
  height: 4px;
  background: #fff;
  border-radius: 50%;
}
.statu .statu_color1{
  background-color: #FFC82C;
}
.statu .statu_color2{
  background-color: #FF88B6;
}
.statu .statu_color3{
  background-color: #00C1FF;
}
.statu .statu_color4{
  background-color: #10cd66;
}

.month_header{
  height: 70rpx;
  line-height: 70rpx;
  border-top: 8rpx solid #F4F4F4;
  padding: 0 40rpx;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:#000000;
  letter-spacing:1px;
}
.currentMonth{
  color:#49C6FF;
}
.small{
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  bottom: 204rpx;
  right: 30rpx;
  border-radius: 50%;
}

