}
.view_right2 .item:nth-child(3n){
  margin-right: 20rpx;
}



.up{
  animation: slideContentUp .3s;
  -webkit-animation:slideContentUp .3s;
}
.down{
  animation: slideContentDown 2s;
  -webkit-animation:slideContentDown 2s;
}
@keyframes slideContentUp {
  from {
    /*设置为正数则底部弹出来，负数则相反*/
    transform: translateY(100%); 
  }
  to {
    transform: translateY(0%);
  }
}
@keyframes slideContentDown {
  from {
    transform: translateY(0%);
  }
  to {
    transform: translateY(100%);
  }
}


