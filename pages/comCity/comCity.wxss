}
.view_right{
  width: 520rpx;
  float: left;
  padding: 0 28rpx 0 30rpx;
  height: 100%;
}
.view_right .item{
  width: 160rpx;
  height: 64rpx;
  line-height: 64rpx;
  background:rgba(244,244,244,1);
  text-align: center;
  font-size:28rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(142,142,142,1);
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.view_right .active{
  color: #49C6FF;
}
.view_right .item:nth-child(3n){
