.item-picker .iconfont {
  font-family: "iconfont" !important;
  /* font-size: 40rpx; */
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.iconleft-arrow:before {
  content: "\e600";
  float: right;
  color: #D4D4D4;
}



