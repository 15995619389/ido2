/* pages/detail/detail.wxss */
.container{height:100%;overflow:hidden;}
.detail-page{
  overflow: hidden;
  width: 100%;
  height: auto;
  /* background: #F4F4F4; */
  background: #fff;
}
.detail-header{
  padding: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  background:rgba(244,244,244,1);
  font-size:36rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.page-info{
  overflow: hidden;
  margin-top: 45px;
}
.detail-item{
  border-bottom: 1px solid #ebebeb;
  padding: 28rpx 30rpx;
}
.item-header{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
}
.item-header .header-left{
  float: left;
  line-height: 60rpx;
  height: 60rpx;
  font-size:32rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(73,198,255,1);
}
.item-header .header-left image{
  width: 60rpx;
  height: 60rpx;
  float: left;
  margin-right: 12rpx;
}
.item-header .header-right{
  float: right;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(166,166,166,1);
  font-size: 24rpx;
}
.item-content{
  padding:10rpx 0;
  overflow: hidden;
}
.content-text{
  font-size:28rpx;
  font-family:PingFang-SC-Regular,PingFang-SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
}
.img1{
  width: 440rpx;
  height: 330rpx;
}
.img2{
  float: left;
  width: 48%;
  height: 230rpx;
}
.img2:first-child{
  margin-right: 2%;
}
.img3{
  width: 32%;
  height: 236rpx;
  float: left;
  margin-right: 2%;
  margin-bottom: 10rpx;
}
.img3:nth-child(3n){
  margin-right: 0;
}
.item-footer{
  overflow: hidden;
  height: 58rpx;
  line-height: 58rpx;
}
.city_left{
  width: auto;
  float: left;
  padding: 0 10rpx 0 40rpx;
  height: 58rpx;
  line-height: 58rpx;
  background: #f4f4f4;
  font-size:24rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(166,166,166,1);
  position: relative;

}
.city_icon{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  width: 22rpx;
  height: 38rpx;
}
.city_right{
  float: right;
  line-height: 58rpx;
  font-size:20rpx;
  font-family:PingFang-SC-Regular,PingFang-SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
/*  */
.page-no-info{
  text-align: center;
  overflow: hidden;
  padding-top: 290rpx;
  font-size: 28rpx;
  color: #949494;
}
.no-info_icon{
  width: 150rpx;
  height: 150rpx;
}
.tip{
  color: #888888;
  font-size: 28rpx;
  padding: 80rpx 0 26rpx;
}
.btn{
  width: 160rpx !important;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 40rpx;
  text-align: center;
  padding: 0 0;
  font-size: 26rpx;
  font-weight: 400;
  border: 1px solid #49c6ff;
  color: #49c6ff;
}

@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1825825_xf5tcl0i22.eot?t=1594006020067'); /* IE9 */
