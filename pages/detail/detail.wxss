  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1825825_xf5tcl0i22.woff?t=1594006020067') format('woff'),
  url('//at.alicdn.com/t/font_1825825_xf5tcl0i22.ttf?t=1594006020067') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1825825_xf5tcl0i22.svg?t=1594006020067#iconfont') format('svg'); /* iOS 4.1- */
